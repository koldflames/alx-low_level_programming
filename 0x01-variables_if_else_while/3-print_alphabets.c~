#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 *main - the entry point of the program
 *Description - prints out sizes of each type
 *Return: 0
 *Description: stops the program
 */
int main(void)
{
char lowercase = 'a';
char uppercase = 'A';
  
// Print lowercase alphabet
while (lowercase <= 'z')
{
putchar(lowercase);
lowercase++;
}
  
// Print uppercase alphabet
while (uppercase <= 'Z')
{
putchar(uppercase);
uppercase++;
}
  
// Print newline character
putchar('\n');
  
 return (0);
}
